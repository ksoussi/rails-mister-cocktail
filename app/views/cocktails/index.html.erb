<% photo_arr = ["https://www.recetasderechupete.com/wp-content/uploads/2016/04/Mojito-copia-525x360.jpg?width=1200&enable=upscale",
  "https://inspiredbycharm.com/wp-content/uploads/2019/10/Maple-Cranberry-Bourbon-Cocktail-Recipe.jpg",
  "http://www.fnstatic.co.uk/images/content/recipe/romeo-and-juliet-cocktail-valentine-s-special.jpg",
  "https://thespicyapron.com/wp-content/uploads/2019/08/Pineapple-Vodka-Cocktail-Horizontal-Martini-Small-1024x684.jpg",
  "https://okdiario.com/img/2018/07/02/receta-de-cocktail-de-martini-con-limon-1-655x368.jpg",
  "data:image/jpeg;base64,",
  "https://253qv1sx4ey389p9wtpp9sj0-wpengine.netdna-ssl.com/wp-content/uploads/2019/09/COM_photocredit_Jens_Johnson_1920x1280.jpg",
  "https://image.businessinsider.com/5a79dacfe21c1125008b4931?width=1100&format=jpeg&auto=webp",
  "https://thespicyapron.com/wp-content/uploads/2019/08/Pineapple-Vodka-Cocktail-Horizontal-Martini-Small-1024x684.jpg",
  "https://keyassets-p2.timeincuk.net/wp/prod/wp-content/uploads/sites/53/2018/06/sex-on-the-beach-recipe.jpg",
  "https://www.nospoonnecessary.com/wp-content/uploads/2016/11/Apple-Gin-Autumn-Cocktail-14-2.jpg",
  "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTEySTuE-BYPd5d-lGIvdUQ8yVVpnJHFUSJ5w2kBGfiQAppZqvj&s",
  "https://inspiredbycharm.com/wp-content/uploads/2019/09/pumpkin-spice-old-fashioned-recipe.jpg",
  "https://images.ctfassets.net/qu53tdnhexvd/6uYjedCwC0VXj539Ho8lB2/cea3490324ffb117f0e30d43704741fb/hugo-spritz-cocktail.jpg"] %>

<% placeholder = "https://image.businessinsider.com/5a79dacfe21c1125008b4931?width=1100&format=jpeg&auto=webp" %>

<div class="banner" style="background-image: linear-gradient(rgba(255,255,255,0.1),rgba(255,255,255,0.1)), url(https://media.istockphoto.com/photos/vintage-retro-grungy-background-design-and-pattern-texture-picture-id656453072?k=6&m=656453072&s=612x612&w=0&h=4TW6UwMWJrHwF4SiNBwCZfZNJ1jVvkwgz3agbGBihyE=);">
  <img id="logo" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQox2s5sTKE0NMfl-Pc4YVD_kNeqzMk22IH5suTV-Vg9M_T49TaOw&s" alt="Mister Cocktail">
  <div class="container">
    <br>
    <div class="pgrid">
      <% @cocktails.each do |ck| %>
        <div class="card-category" >
          <div class="card-category-inner">
            <div class="card-category-front" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(<%= photo_arr[ck.id-1] %>)">
              <%= ck.name %>
            </div>
            <div class="card-category-back">
                <% ck.ingredients.each_with_index do |ig, ix| %>
                  <p><%= ig.name.capitalize %> (<%= Dose.where(cocktail_id: ck.id, ingredient_id: ig.id).to_a[0].description %>) <%= link_to cocktail_dose_path(ck.id, Dose.where(cocktail_id: ck.id, ingredient_id: ig.id).to_a[0].id),
                  method: :delete,
                  data: { confirm: "Are you sure?" } do %>
                    <i class="far fa-trash-alt"></i>
                    <% end %>
                  </p>
              <% end %>
              <hr>
              <p>
                <%= link_to new_cocktail_dose_path(ck.id) do %>
                  <i class="fas fa-plus"></i> Add a new dose
                <% end %>
              </p>
            </div>
          </div>
        </div>
        <% end %>
    </div>
    <br>
    <div class="center-this">
      <%= link_to "Add a new cocktail", new_cocktail_path, class: "btn btn-light"%>
    </div>
  </div>
</div>
